{
  "name": "@slimio/winelog",
  "version": "1.0.0",
  "description": "Windows Events log reader - low-level C++/Node.js N-API binding",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "prebuilds": "prebuildify --napi",
    "build": "cross-env node-gyp configure && node-gyp build",
    "test": "cross-env psp && ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Winelog.git"
  },
  "keywords": [
    "SlimIO",
    "windows",
    "event",
    "events",
    "logs",
    "log",
    "low-level",
    "natif",
    "reader",
    "binding",
    "napi",
    "n-api",
    "microsoft",
    "eventlog",
    "monitoring",
    "security",
    "application",
    "system"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "binding.gyp",
    "winelog.cpp",
    "include",
    "prebuilds"
  ],
  "engines": {
    "node": ">=13"
  },
  "author": "SlimIO",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/SlimIO/Winelog/issues"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "homepage": "https://github.com/SlimIO/Winelog#readme",
  "dependencies": {
    "node-addon-api": "^2.0.0",
    "node-gyp-build": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^4.0.0",
    "@slimio/is": "^1.5.1",
    "@slimio/psp": "^0.11.0",
    "ava": "3.2.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.1.0",
    "husky": "^4.2.1",
    "jsdoc": "^3.6.2",
    "node-gyp": "^6.0.1",
    "pkg-ok": "^2.3.1",
    "prebuildify": "^3.0.0"
  }
}
